<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Filters>
        <MarkerFilter marker="ControlFlowVariableRenames" onMatch="DENY" onMismatch="ACCEPT"/>
    </Filters>
    <Appenders>
        <Console name="stderr" target="SYSTEM_ERR">
            <PatternLayout pattern="%d{ISO8601} [%-5p] in %c: %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="stderr"/>
        </Root>
    </Loggers>
</Configuration>
